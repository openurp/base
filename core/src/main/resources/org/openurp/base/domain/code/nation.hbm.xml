<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.base.domain.code"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    <class name="GenderBean" entity-name="org.openurp.base.code.Gender">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
        <property name="code" length="20" unique="true" not-null="true"/>
        <property name="name" length="20" unique="true" not-null="true"/>
        <property name="enName" length="100"/>
        <property name="beginOn"/>
        <property name="endOn"/>
        <property name="updatedAt"/>
        <property name="remark" length="30"/>
    </class>

    <class name="NationBean" entity-name="org.openurp.base.code.Nation">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
        <property name="code" length="20" unique="true" not-null="true"/>
        <property name="name" length="60" unique="true" not-null="true"/>
        <property name="enName" length="200"/>
        <property name="alphaCode" length="3" unique="true" not-null="true"/>
        <property name="beginOn"/>
        <property name="endOn"/>
        <property name="updatedAt"/>
        <property name="remark" length="30"/>
    </class>

    <class name="CountryBean" entity-name="org.openurp.base.code.Country">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
        <property name="code" length="20" unique="true" not-null="true"/>
        <property name="name" length="200" unique="true" not-null="true"/>
        <property name="enName" length="200"/>
        <property name="alpha2Code" length="2" unique="true" not-null="true"/>
        <property name="alpha3Code" length="3" unique="true" not-null="true"/>
        <property name="beginOn"/>
        <property name="endOn"/>
        <property name="updatedAt"/>
        <property name="remark" length="30"/>
    </class>
    
    <class name="PoliticalAffiliationBean" entity-name="org.openurp.base.code.PoliticalAffiliation">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
        <property name="code" length="20" unique="true" not-null="true"/>
        <property name="name" length="200" unique="true" not-null="true"/>
        <property name="enName" length="200"/>
        <property name="beginOn"/>
        <property name="endOn"/>
        <property name="updatedAt"/>
        <property name="remark" length="30"/>
    </class>

    <class name="LanguageBean" entity-name="org.openurp.base.code.Language">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
        <property name="code" length="20" unique="true" not-null="true"/>
        <property name="name" length="200" unique="true" not-null="true"/>
        <property name="enName" length="200"/>
        <property name="beginOn"/>
        <property name="endOn"/>
        <property name="updatedAt"/>
        <property name="remark" length="30"/>
    </class>

    <class name="DivisionBean" entity-name="org.openurp.base.code.Division">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
        <property name="code" length="20" unique="true" not-null="true"/>
        <property name="name" length="200" not-null="true"/>
        <property name="enName" length="200"/>
        <property name="beginOn"/>
        <property name="endOn"/>
        <property name="updatedAt"/>
        <property name="remark" length="30"/>
    </class>

  <class name="EducationBean" entity-name="org.openurp.base.code.Education">
    <cache usage="read-write"/>
    <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
    <property name="code" length="20" unique="true" not-null="true"/>
    <property name="name" length="200" unique="true" not-null="true"/>
    <property name="enName" length="200"/>
    <property name="beginOn"/>
    <property name="endOn"/>
    <property name="updatedAt"/>
    <property name="remark" length="30"/>
  </class>
  
  <class name="FamilyRelationBean" entity-name="org.openurp.base.code.FamilyRelation">
    <cache usage="read-write"/>
    <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
    <property name="code" length="20" unique="true" not-null="true"/>
    <property name="name" length="200" unique="true" not-null="true"/>
    <property name="enName" length="200"/>
    <property name="beginOn"/>
    <property name="endOn"/>
    <property name="updatedAt"/>
    <property name="remark" length="30"/>
  </class>
</hibernate-mapping>