<?xml version="1.0"?>
<mvc>
  <profile name="openurp-base-ws" package="org.openurp.base.ws">
    <action suffix="WS" defaultMethod="index" />
    <url path="/" suffix="" style="plur-seo" />
    <interceptors>
      <interceptor name="web.Interceptor.hibernate" />
      <interceptor name="web.Interceptor.cors" />
    </interceptors>
  </profile>

  <profile name="openurp-base-ws-edu" package="org.openurp.base.ws.edu">
    <action suffix="WS" defaultMethod="index" />
    <url path="/edu/{project}" suffix="" style="plur-seo" />
    <interceptors>
      <interceptor name="web.Interceptor.hibernate" />
      <interceptor name="web.Interceptor.cors" />
    </interceptors>
  </profile>

  <profile name="openurp-base-ws-std" package="org.openurp.base.ws.std">
    <action suffix="WS" defaultMethod="index" />
    <url path="/std/{project}" suffix="" style="plur-seo" />
    <interceptors>
      <interceptor name="web.Interceptor.hibernate" />
      <interceptor name="web.Interceptor.cors" />
    </interceptors>
  </profile>

  <profile name="openurp-base-ws-profile" package="org.openurp.base.ws.profile">
    <action suffix="WS" defaultMethod="index" />
    <url path="/profile" suffix="" style="plur-seo" />
    <interceptors>
      <interceptor name="web.Interceptor.hibernate" />
      <interceptor name="web.Interceptor.cors" />
    </interceptors>
  </profile>
</mvc>
